		<script src="../static/js/plugins/pace/pace.min.js"></script>

		<script>
			$('body.canvas-menu .sidebar-collapse').slimScroll({
				height: '100%',
				railOpacity: 0.9
			});	
			$(function() {
				$("#go_submit").click(function() {
/*						window.alert("test");*/
					var a = document.getElementById("selective").value;
					if(a==""||a==null){
						window.alert("部门名称或编号不能为空！");
						return false;
					}
				});
			});
		</script>
